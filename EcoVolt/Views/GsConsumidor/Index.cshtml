@model IEnumerable<EcoVolt.Models.GsConsumidor>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.EnergiaAtribuidaKwh)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NivelPrioridade)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TipoConsumidor)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Localizacao)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.EnergiaAtribuidaKwh)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NivelPrioridade)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TipoConsumidor.IdTipoConsumidor)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Localizacao.IdLocalizacao)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdConsumidor">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdConsumidor">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdConsumidor">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
